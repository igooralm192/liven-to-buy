x.tsx",["143"],"import React, { useEffect, useState } from 'react'\nimport { useDispatch, useSelector } from 'react-redux'\nimport { FaShoppingCart } from 'react-icons/fa'\n\nimport './styles.css'\n\nimport { Link } from 'react-router-dom'\nimport ProductsList from './ProductsList'\n\nimport { AppState } from '../../store'\nimport { Product } from '../../store/products/types'\nimport { getProducts } from '../../store/products/thunks'\nimport { addProduct } from '../../store/cart/actions'\n\nconst Products: React.FC = () => {\n  const dispatch = useDispatch()\n\n  const productsById = useSelector((state: AppState) => state.products.byId)\n  // const selectedProducts = useSelector((state: AppState) => state.cart.products)\n\n  const [products, setProducts] = useState<Product[]>([])\n\n  function handleAddProduct(productId: string) {\n    dispatch(addProduct(productId))\n  }\n\n  useEffect(() => {\n    dispatch(getProducts())\n  }, [])\n\n  useEffect(() => {\n    const keys = Object.keys(productsById)\n\n    setProducts(keys.map(key => productsById[key]))\n  }, [productsById])\n\n  // console.log(selectedProducts)\n\n  return (\n    <main id=\"products-container\" className=\"content\">\n      <h3 className=\"products title\">Our products</h3>\n\n      <ProductsList products={products} onAddProduct={handleAddProduct} />\n\n      <Link to=\"/cart\">\n        <button type=\"button\" className=\"products cart btn\">\n          <FaShoppingCart />\n        </button>\n      </Link>\n    </main>\n  )\n}\n\nexport default Products\n","/home/iguzinhotv/Documentos/Codigos/Liven/liven-to-buy/src/routes/ProtectedRoute.tsx",[],"/home/iguzinhotv/Documentos/Codigos/Liven/liven-to-buy/src/api/products/index.ts",[],"/home/iguzinhotv/Documentos/Codigos/Liven/liven-to-buy/src/store/index.ts",[],"/home/iguzinhotv/Documentos/Codigos/Liven/liven-to-buy/src/store/products/reducer.ts",[],"/home/iguzinhotv/Documentos/Codigos/Liven/liven-to-buy/src/store/products/types.ts",[],"/home/iguzinhotv/